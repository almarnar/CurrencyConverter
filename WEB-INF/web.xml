<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>rep</param-name>
        <param-value>Report.html</param-value>
    </context-param>
    <listener>
        <listener-class>MyServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Converter</servlet-name>
        <servlet-class>Converter</servlet-class>
        <init-param>
            <param-name>Par1</param-name>
            <param-value>http://www.ecb.int/stats/eurofxref/eurofxref-daily.xml</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Code</servlet-name>
        <servlet-class>Code</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Report5</servlet-name>
        <servlet-class>Report5</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Converter</servlet-name>
        <url-pattern>/Converter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Code</servlet-name>
        <url-pattern>/Code</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Report5</servlet-name>
        <url-pattern>/Report5</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
